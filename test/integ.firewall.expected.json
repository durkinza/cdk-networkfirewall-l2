    "MyStateful5TupleRuleGroupE35C4D46": {
      "Type": "AWS::NetworkFirewall::RuleGroup",
      "Properties": {
        "Capacity": 100,
        "RuleGroupName": "MyStateful5TupleRuleGroup",
        "Type": "STATEFUL",
        "RuleGroup": {
          "RuleVariables": {
            "IPSets": {
              "HOME_NET": {
                "Definition": [
                  "10.0.0.0/16",
                  "10.10.0.0/16"
                ]
              }
            },
            "PortSets": {
              "WEB_PORTS": {
                "Definition": [
                  "443",
                  "80"
                ]
              }
            }
          },
          "RulesSource": {
            "StatefulRules": [
              {
                "Action": "DROP",
                "Header": {
                  "Destination": "$HOME_NET",
                  "DestinationPort": "$WEB_PORTS",
                  "Direction": "FORWARD",
                  "Protocol": "TCP",
                  "Source": "10.10.0.0/16",
                  "SourcePort": "any"
                },
                "RuleOptions": [
                  {
                    "Keyword": "sid",
                    "Settings": [
                      "1234"
                    ]
                  }
                ]
              }
            ]
          },
          "StatefulRuleOptions": {
            "RuleOrder": "STRICT_ORDER"
          }
        }
      }
    },
    "MyStatefulDomainListRuleGroup481BB765": {
      "Type": "AWS::NetworkFirewall::RuleGroup",
      "Properties": {
        "Capacity": 100,
        "RuleGroupName": "MyStatefulDomainListRuleGroup",
        "Type": "STATEFUL",
        "RuleGroup": {
          "RuleVariables": {},
          "RulesSource": {
            "RulesSourceList": {
              "GeneratedRulesType": "DENYLIST",
              "TargetTypes": [
                "TLS_SNI",
                "HTTP_HOST"
              ],
              "Targets": [
                ".example.com",
                "www.example.org"
              ]
            }
          },
          "StatefulRuleOptions": {
            "RuleOrder": "STRICT_ORDER"
          }
        }
      }
    },
    "MyStatefulSuricataRuleGroupA19EA3D1": {
      "Type": "AWS::NetworkFirewall::RuleGroup",
      "Properties": {
        "Capacity": 100,
        "RuleGroupName": "MyStatefulSuricataRuleGroup",
        "Type": "STATEFUL",
        "RuleGroup": {
          "RuleVariables": {
            "IPSets": {
              "HTTP_SERVERS": {
                "Definition": [
                  "10.0.0.0/16"
                ]
              }
            },
            "PortSets": {
              "HTTP_PORTS": {
                "Definition": [
                  "80",
                  "8080"
                ]
              }
            }
          },
          "RulesSource": {
            "RulesString": "alert tcp $EXTERNAL_NET any -> $HTTP_SERVERS $HTTP_PORTS (msg:\".htpasswd access attempt\"; flow:to_server,established; content:\".htpasswd\"; nocase; sid:210503; rev:1;)"
          },
          "StatefulRuleOptions": {
            "RuleOrder": "STRICT_ORDER"
          }
        }
      }
    },
